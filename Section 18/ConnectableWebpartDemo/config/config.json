{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "provider-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/providerWebpart/ProviderWebpartWebPart.js",
          "manifest": "./src/webparts/providerWebpart/ProviderWebpartWebPart.manifest.json"
        }
      ]
    },
    "consumer-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/consumerWebpart/ConsumerWebpartWebPart.js",
          "manifest": "./src/webparts/consumerWebpart/ConsumerWebpartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ProviderWebpartWebPartStrings": "lib/webparts/providerWebpart/loc/{locale}.js",
    "ConsumerWebpartWebPartStrings": "lib/webparts/consumerWebpart/loc/{locale}.js"
  }
}
